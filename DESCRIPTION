Package: flm.NE.WNV
Type: Package
Title: Functional Linear Modeling For Vector-borne Disease
Version: 0.3.0
Authors@R: c(person("Kelly Helm", "Smith", role = c("aut", "cre"),
           email = "ksmith2@unl.edu"), person("Andrew", "Tyre",
           role = c("aut"), email = "atyre2@unl.edu"), 
           person("Alexander Sasha","Keyel", role = c("ctb"), 
